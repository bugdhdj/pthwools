{
  "name": "bugdhdj/pthwools",
  "type": "library",
  "description": "A polyfill for pthreads",
  "keywords": [
    "pthreads",
    "polyfill",
    "swoole",
    "pthwools"
  ],
  "homepage": "http://pthreads.org/",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Joe Watkins",
      "email": "krakjoe@php.net"
    },
    {
      "name": "William Wang",
      "email": "not@dhdj.org"
    },
    {
      "name": "LixWorth",
      "email": "lixworth@outlook.com"
    }
  ],
  "require": {
    "php": "^7.4 | ^8.0",
    "ext-swoole": "^4.0"
  },
  "autoload": {
    "files": [
      "src/Constant/Constant.php"
    ],
    "psr-4": {
      "": [
        "src",
        "src/Interface"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "eaglewu/swoole-ide-helper": "dev-master"
  }
}
